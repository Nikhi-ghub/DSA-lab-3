#include <stdio.h>

int main() {
    int arr[100];
    int n, i, j, count = 0;
    printf("Enter number of elements in the array: ");
    scanf("%d", &n);
    printf("Enter %d elements:\n", n);
  
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    for(i = 0; i < n; i++) {
      
        for(j = i + 1; j < n; j++) {
            
            if(arr[i] == arr[j]) {
                count++;
                break; 
            }
        }
    }

    printf("\nTotal number of duplicate elements found = %d\n", count);

    return 0;
}
